<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="rNascar23Multi.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:rNascar23Multi"
    xmlns:pages="clr-namespace:rNascar23Multi.Pages">

    <ShellContent Title="Leaderboard" Route="Leaderboard" ContentTemplate="{DataTemplate pages:MainPage}" />
    <ShellContent Title="Pit Stops" Route="PitStops" ContentTemplate="{DataTemplate pages:PitStopsPage}" />
    <ShellContent Title="Schedules" Route="Schedules" ContentTemplate="{DataTemplate pages:SchedulesPage}" />
    <ShellContent Title="Settings" Route="Settings" ContentTemplate="{DataTemplate pages:SettingsPage}" />
    <ShellContent Title="Exit" Route="Exit" ContentTemplate="{DataTemplate pages:QuitPage}"/>

</Shell>
